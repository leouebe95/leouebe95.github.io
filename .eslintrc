{
    "parserOptions": {
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": false
        }
    },
	"extends": "eslint:recommended",
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "console": false,
        "JSON": false
    },
    "rules": {
        "no-mixed-spaces-and-tabs": 0,
        semi: ["error", "always"],
        "no-bitwise": 2,
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "comma-dangle": ["error", "never"],
        "camelcase": 0,
        "curly": 2,
        "no-debugger": 0,
        "eqeqeq": 2,
        "no-eq-null": 0,
        "no-eval": 0,
        "no-unused-expressions": 2,
        "guard-for-in": 0,
        "block-scoped-var": 0,
        "strict": [
            2,
            "function"
        ],
        "wrap-iife": [
            2,
            "any"
        ],
        "no-iterator": 0,
        "no-use-before-define": 0,
        "linebreak-style": 0,
        "comma-style": [
            2,
            "last"
        ],
        "no-loop-func": 0,
        "no-multi-str": 0,
        "new-cap": ["error", { "properties": false }],
        "no-caller": 2,
        "no-empty": 2,
        "no-new": 2,
        "no-plusplus": 0,
        "no-proto": 0,
        "no-undef": 2,
        "no-unused-vars": ["error", { "args": "none" }],
        "no-script-url": 0,
        "no-shadow": 0,
        "dot-notation": 2,
        "no-new-func": 0,
        "no-new-wrappers": 0,
        "no-invalid-this": 0,
        "no-with": 0,
        "indent": 0,
        "quotes": [
            2,
            "double",
            {
                "avoidEscape": true
            }
        ],
        "max-statements": 0,
        "max-depth": 0,
        "valid-jsdoc": "error",
        "require-jsdoc": ["error", {
            "require": {
                "FunctionDeclaration": false,
                "MethodDefinition": false,
                "ClassDeclaration": true
            }
        }]
    }
}
